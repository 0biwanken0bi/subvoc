{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true,
            "jsx": true
        }
    },
    "rules": {
        "semi": ["error", "always"],
        "no-unused-vars": 0
    }
}
